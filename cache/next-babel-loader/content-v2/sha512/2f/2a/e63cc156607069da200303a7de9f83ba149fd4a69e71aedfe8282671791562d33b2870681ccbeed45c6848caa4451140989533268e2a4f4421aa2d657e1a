    children: /*#__PURE__*/_jsx(Form, {\n        formikConfig: {\n          initialValues: FORMIK.voucher.values,\n          validationSchema: FORMIK.voucher.validationSchema(locale),\n          onSubmit: function onSubmit(values) {\n            addToCart(values);\n          }\n        },\n        types: FORMIK.voucher.types,\n        placeholders: FORMIK.voucher.placeholders(locale),\n        suffixes: FORMIK.voucher.suffixes,\n        buttonTitle: TRANSLATE[locale].addToCart\n      })\n    })]\n  });\n};\n\nexport default Voucher;","map":null,"metadata":{},"sourceType":"module"}